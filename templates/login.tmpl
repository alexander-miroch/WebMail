	<tr><td align="right" class="ll"><strong>Пароль:</strong></td><td align="left" class="ll"><input name="password" type="password" maxlength="20" size="28" class="vl" /></td></tr>
	<tr><td colspan="2" align="center">
		<input type="hidden" name="page" value="@<<x_page>>@" />
		<input type="submit" value="Вход" class="button">
		</table>

	</td></tr>
	<tr><td><img src="/images/e2.jpg" style=/></td><td width="100%">&nbsp;</td><td align="right"><img src="/images/e3.jpg"></td></tr>
	</td></tr>
	</form>
	</table>

 </td></tr>
</table>
</body>
</html>

